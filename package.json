{
  "name": "zoster",
  "version": "0.2.4",
  "description": "Android, ios, url, deeplinking, test, appium, adb, electronjs, native, chromium",
  "scripts": {
    "server": "npm run build && ./bin/zoster --neutron",
    "prestart": "standard && npm run build",
    "start": "./bin/zoster",
    "build": "standard && ./node_modules/babel-cli/bin/babel.js lib -d node && ./link.js",
    "install": "npm run build",
    "test": "standard && ./node_modules/mocha/bin/mocha tests/* --compilers js:babel-core/register -t 15s",
    "examples-tests": "babel-node tests/test_examples.js",
    "prepublish": "npm run build"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Urucas/zoster.git"
  },
  "bin": {
    "zoster": "./bin/zoster"
  },
  "bugs": {
    "url": "https://github.com/Urucas/zoster/issues"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "homepage": "https://github.com/Urucas/zoster",
  "author": "Urucas <hello@urucas.com>",
  "license": "MIT",
  "dependencies": {
    "adbjs": "^0.1.3",
    "angular": "^1.3.15",
    "appium-running": "^0.1.2",
    "assert": "^1.3.0",
    "babel": "^5.1.10",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "bootstrap": "^3.3.4",
    "colors": "^1.1.2",
    "express": "^4.12.3",
    "internal-ip": "^1.0.0",
    "jquery": "^2.1.3",
    "mocha": "^2.5.3",
    "multer": "^0.1.8",
    "node-idevice": "^0.1.5",
    "openurl": "^1.1.0",
    "optimist": "^0.6.1",
    "socket.io": "^1.3.5",
    "vd": "^0.6.0",
    "wd": "^0.3.11",
    "zeroclipboard": "^2.2.0"
  },
  "devDependencies": {
    "appium-adb": "^2.9.3",
    "babel-cli": "^6.18.0",
    "babelify": "^6.1.2",
    "mocha": "^2.2.5",
    "mocha-clean": "^0.4.0",
    "standard": "^8.6.0"
  },
  "standard" : {
    "ignore" : [
      "node/*",
      "public/*",
      "examples/*"
    ],
    "globals": [
      "describe",
      "it",
      "colors"
    ]
  }
}
